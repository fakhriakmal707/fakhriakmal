#include <iostream>
using namespace std;
int main(){
  int r,jumlah, max, min;
  int nilai[10]={40,5,88,17,76,67,79,90,65,50};

  for(int r=0; r<10;r++){
    r=nilai[r];

    if(r==1){
      min = r;
      max = r;
    }
    else if(min>r){
      min = r;
    }
    else if(max<r){
      max = r;
    }
    else{}
  }
  cout<<endl;
  cout<< "Nilai Terkecil : "<< min <<endl;
  cout<< "Nilai Terbesar : "<< max <<endl;
  
}
